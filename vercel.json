{
  "version": 2,
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/build/server/index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/build/server/index.js"
    },
    {
      "src": "/webhooks/(.*)",
      "dest": "/build/server/index.js"
    },
    {
      "src": "/apps/(.*)",
      "dest": "/build/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}